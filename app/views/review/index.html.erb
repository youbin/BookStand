<h1> Listing Reviews </h1>

<table>
  <thead>
    <tr>
      <th>key</th>
      <th>u_id</th>
      <th>r_review</th>
      <th>r_starPoint</th>
      <th>r_time</th>
      <th>cm_ids</th>
    </tr>
  </thead>

  <tbody>
    <% @reviews.each do |review| %>
      <tr>
        <td> </td>
        <td><%= review["key"] %></td>
        <td><%= review["u_id"] %></td>
        <td><%= review["r_reivew"] %></td>
        <td><%= review["r_starPoint"] %></td>
        <td><%= review["r_time"] %></td>
        <td><%= review["cm_ids"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
