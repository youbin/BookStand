<h1> Listing Feeds </h1>

<table>
  <thead>
    <tr>
      <th>key</th>
      <th>type</th>
      <th>u_id</th>
      <th>b_id</th>
      <th>r_id</th>
      <th>cm_id</th>
      <th>fr_id</th>
      <th>f_time</th>
    </tr>
  </thead>

  <tbody>
    <% @feeds.each do |feed| %>
      <tr>
        <td> </td>
        <td><%= feed["key"] %></td>
        <td><%= feed["type"] %></td>
        <td><%= feed["u_id"] %></td>
        <td><%= feed["b_id"] %></td>
        <td><%= feed["r_id"] %></td>
        <td><%= feed["cm_id"] %></td>
        <td><%= feed["fr_id"] %></td>
        <td><%= feed["f_time"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
